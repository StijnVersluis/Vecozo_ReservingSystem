@model List<WorkzoneViewModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "";
}
@foreach (var workzone in Model)
{
    <a class="list-group-item" data-toggle="modal" data-target="#WorkzoneSelectedModal" data-workzone-id="@workzone.Id" data-workzone-name="@workzone.Name">
        <div class="row">
            <div class="col-6">
                @workzone.Name
            </div><div class="col-6">
                @workzone.Workspaces
            </div>
        </div>
    </a>
}